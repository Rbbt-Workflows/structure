structure=/$PWD filter="*.rb *.R *.js *.sass *.haml *.tsv" {
 workflow.rb
 workflow.md
 test_workflow.rb
 ToDo.md
 doc=doc{
  methods.md
 }
 lib=lib {
  sinatra.rb
  entity=rbbt/entity{
   structure.rb
  }
  structure=structure{
   annotator.rb
   ssw.rb
   alignment.rb

   pdb_helper.rb
   pdb_alignment.rb

   neighbours.rb

   interactome_3d.rb
   uniprot.rb
   interpro.rb
   COSMIC.rb
   appris.rb

   sinatra.rb

   workflow=workflow{
    alignments.rb
    annotate.rb
    helpers.rb
    util.rb
    wizard.rb
   }
  }

 }
 share=share{
  rbbt_commands=rbbt_commands filter="*"{
   bootstrap
   sample_report
  }
  software=software/opt/ssw filter="*"{
   ssw.c
   ssw.h
  }
 }
 test=test{
  test_ssw.rb
  test_uniprot.rb
 }

 www=www {
  views=views {
   tasks.haml
   compass=compass{
    protein.sass
   }
   public=public{
    js=js{
     jmol.js
     isoform_svg.js
     protein_tool.js
     rbbt.protein_tool.js
     secondary_structure.js
     sequence.js
     jquery-jmol=jquery-jmol{
      jmol-accent.js
      jmol-accent.save.js
      jquery.jmol.js
     }
    }
   }
   tools=tools{
    protein_tool.haml
    protein_tool=protein_tool{
     COSMIC_tab.haml
     PDB-Viewer_tab.haml
     controls.haml
    }
   }
   entity=entity{
    MutatedIsoform.haml
    MutatedIsoform=MutatedIsoform{
     affected_features.haml
    }
    Protein.haml
    Protein=Protein{
     COSMIC.haml
     COSMIC_neighbours.haml
    }

    Study=Study{
     interfaces.haml
    }
   }
   partials=partials{
    download.haml
    isotope_form.haml
    protein_mutation_overview.haml
   }
   layout=layout{
    top_menu.haml
   }
   Structure=Structure{
    tasks.haml
    scores=scores{
     job_result.haml
    }
    score_summary=score_summary{
     job_result.haml
    }
   }
  }
 }
}
